{
    "name": "rahulalam31/larabot",
    "description": "Laravel Chatbot package",
    "keywords": [
        "Rahul Alam",
        "laravel",
        "NLP",
        "openAI",
        "laravel chatbot",
        "larabot"
    ],
    "homepage": "https://github.com/rahulalam31/larabot",
    "license": "MIT",
    "authors": [
        {
            "name": "Rahul Alam",
            "email": "rahulalam31@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "rahulalam31\\Larabot\\": "src/",
            "rahulalam31\\Larabot\\Database\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "rahulalam31\\Larabot\\LarabotServiceProvider"
            ],
            "aliases": {
                "Larabot": "rahulalam31\\Larabot\\Facades\\Larabot"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
